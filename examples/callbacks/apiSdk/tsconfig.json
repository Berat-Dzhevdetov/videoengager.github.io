{
    "compilerOptions": {
      "outDir": "./javascript",
      "noImplicitAny": true,
      "module": "ES2020",
      "target": "ES2020",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "sourceMap": true,
      "noImplicitReturns": true,
      // "composite": true,
      "declaration": true,
      "declarationMap": true,
    }, "exclude": ["node_modules", "dist/browser", "webpack.config.js", "javascript"]
  }